RewriteEngine On
RewriteBase /
RewriteRule ^([0-9]+)/(.*?).html$ /index.php?path=category&id=$1&page=1 [L,QSA]
RewriteRule ^([0-9]+)/([0-9]+)/(.*?).html$ /index.php?path=category&id=$1&page=$2 [L,QSA]
RewriteRule ^([0-9]+)/([0-9]+)-(.*?)-([0-9]+)/(.*?).html$ /index.php?path=article&id=$1 [L,QSA]
RewriteRule ^admin/(.*?).html$ /index.php?path=admin/index&adminPath=$1 [L,QSA]
RewriteRule ^admin$ /index.php?path=admin/index [L,QSA]